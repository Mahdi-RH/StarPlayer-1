<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/onlineMusicsListFragment">

    <fragment
        android:id="@+id/musicsListFragment"
        android:name="com.worldsnas.starplayer.view.musics_list.MusicsListFragment"
        android:label="fragment_musics_list"
        tools:layout="@layout/fragment_musics_list" >
        <action
            android:id="@+id/action_musicsListFragment_to_playerFragment"
            app:destination="@id/playerFragment" />
    </fragment>
    <fragment
        android:id="@+id/playerFragment"
        android:name="com.worldsnas.starplayer.view.PlayerFragment"
        android:label="fragment_player"
        tools:layout="@layout/fragment_player" />
    <fragment
        android:id="@+id/onlineMusicsListFragment"
        android:name="com.worldsnas.starplayer.view.online_music_list.OnlineMusicListFragment"
        android:label="fragment_online_musics_list"
        tools:layout="@layout/fragment_online_musics_list" >
        <action
            android:id="@+id/action_onlineMusicsListFragment_to_playerFragment"
            app:destination="@id/playerFragment" />
    </fragment>
</navigation>